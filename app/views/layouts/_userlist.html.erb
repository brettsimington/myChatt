<div class="userlist">
  <% if current_user %>
<div class="card" style="width: 18rem;">
  <div class="card-header">
    Users Online
  </div>
  <ul class="list-group list-group-flush">
    
   <% @users.each do |user| %>
    <li class="list-group-item"> <%= user.email %> </li>
    <% end %>

  </ul>
</div>


<br />

<div class="card" style="width: 18rem;">
  <div class="card-header">
    Users Offline
  </div>
  <ul class="list-group list-group-flush">
    <li class="list-group-item">Dan</li>
  </ul>
</div>
<% end %>
</div>